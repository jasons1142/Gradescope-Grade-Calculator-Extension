{
    "manifest_version": 3,
    "name": "Gradescope Grade Calculator",
    "version": "1.0.0",
    "description": "Val and Jason's Grade Calculator Extension",
    "default_locale": "en",
    "icons": {
        "16": "Calculator Icon.png",
        "48": "Calculator Icon.png",
        "128": "Calculator Icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "Calculator Icon.png",
            "48": "Calculator Icon.png",
            "128": "Calculator Icon.png"
        },
        "default_title": "Grade Calculator"
    },
    "content_scripts":[
        {
            "matches":[
                "*://*.gradescope.com/courses/*"
            ],
            "js":[
                "contentScript.js"
            ]
        }
    ],
    "permissions":[
        "tabs",
        "activeTab",
        "webNavigation",
        "scripting"
    ]
}
