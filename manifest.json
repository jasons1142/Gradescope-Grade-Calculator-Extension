{
    "manifest_version": 3,
    "name": "Gradescope Grade Calculator",
    "version": "0.0.1",
    "description": "Val and Jason's Grade Calculator Extension",
    "default_locale": "en",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_popup": "NotOnGradescopePopup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        },
        "default_title": "Grade Calculator"
    },
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "contentScript.js"
            ]
        }
    ],
    "permissions":[
        "tabs",
        "activeTab",
        "webNavigation",
        "scripting"
    ],
    "background":{
        "service_worker": "background.js"
    }
}
